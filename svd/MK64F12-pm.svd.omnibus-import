  let UART5_S1_OR_No_0 $bool = no // No overrun has occurred since the last time the flag was cleared.
  let UART5_S1_OR_Overrun_1 $bool = yes // Overrun has occurred or the overrun flag has not been cleared since the last overrun occured.

