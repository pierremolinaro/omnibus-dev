  TCD11_DOFF @offset 0x1174
  TCD12_DOFF @offset 0x1194
  TCD13_DOFF @offset 0x11b4
  TCD14_DOFF @offset 0x11d4
  TCD15_DOFF @offset 0x11f4 $u16 {
