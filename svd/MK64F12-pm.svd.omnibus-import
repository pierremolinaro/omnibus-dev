  let SIM_SOPT1CFG_UVSWE_SOPT1_0 $bool = no // SOPT1 USBVSTBY cannot be written.
  let SIM_SOPT1CFG_UVSWE_SOPT1_1 $bool = yes // SOPT1 USBVSTBY can be written.

