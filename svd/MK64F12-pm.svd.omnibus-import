  TCD9_DADDR @offset 0x1130
  TCD10_DADDR @offset 0x1150
  TCD11_DADDR @offset 0x1170
  TCD12_DADDR @offset 0x1190
  TCD13_DADDR @offset 0x11b0
  TCD14_DADDR @offset 0x11d0
  TCD15_DADDR @offset 0x11f0 $u32 {
