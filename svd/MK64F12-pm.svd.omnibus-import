//--- Enumerated values for register CAU_LDR_CASR, field IC
  let CAU_CAU_LDR_CASR_IC_No_0 $bool = no // No illegal commands issued
  let CAU_CAU_LDR_CASR_IC_Illegal_1 $bool = yes // Illegal command issued

