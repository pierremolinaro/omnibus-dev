  let AIPS1_PACRP_SP0_This_1 $bool = yes // This peripheral requires supervisor privilege level for accesses.

//--- Enumerated values for register PACRU, field TP1
  let AIPS1_PACRU_TP1_Accesses_0 $bool = no // Accesses from an untrusted master are allowed.
  let AIPS1_PACRU_TP1_Accesses_1 $bool = yes // Accesses from an untrusted master are not allowed.

