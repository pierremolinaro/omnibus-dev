  let UART2_S2_RWUID_S1_IDLE__0 $bool = no // S1[IDLE] is not set upon detection of an idle character.
  let UART2_S2_RWUID_S1_IDLE__1 $bool = yes // S1[IDLE] is set upon detection of an idle character.

