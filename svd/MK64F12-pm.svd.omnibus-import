  let UART3_BDH_RXEDGIE_Hardware_0 $bool = no // Hardware interrupts from RXEDGIF disabled using polling.
  let UART3_BDH_RXEDGIE_RXEDGIF_1 $bool = yes // RXEDGIF interrupt request enabled.

