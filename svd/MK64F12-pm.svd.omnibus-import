  let SDHC_VENDOR_EXTDMAEN_In_0 $bool = no // In any scenario, SDHC does not send out the external DMA request.
  let SDHC_VENDOR_EXTDMAEN_When_1 $bool = yes // When internal DMA is not active, the external DMA request will be sent out.

