  CAU_LDR_CA1 @offset 0x84c $u32 {
    CA1:32 // at 0: CA1
  }

//---  Register CAU_LDR_CA2: General Purpose Register 2 - Load Register command
  CAU_LDR_CA2 @offset 0x850 $u32 {
    CA2:32 // at 0: CA2
  }

