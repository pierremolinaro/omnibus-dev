  let UART2_CFIFO_TXOFE_TXOF_0 $bool = no // TXOF flag does not generate an interrupt to the host.
  let UART2_CFIFO_TXOFE_TXOF_1 $bool = yes // TXOF flag generates an interrupt to the host.

