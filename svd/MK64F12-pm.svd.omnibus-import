  let UART1_C5_LBKDDMAS_If_0 $bool = no // If BDH[LBKDIE] and S2[LBKDIF] are set, the LBKDIF interrupt signal is asserted to request an interrupt service.
  let UART1_C5_LBKDDMAS_If_1 $bool = yes // If BDH[LBKDIE] and S2[LBKDIF] are set, the LBKDIF DMA request signal is asserted to request a DMA transfer.

