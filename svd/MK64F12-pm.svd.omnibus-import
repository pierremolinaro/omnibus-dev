  let SIM_SOPT2_RTCCLKOUTSEL_RTC_0 $bool = no // RTC 1 Hz clock is output on the RTC_CLKOUT pin.
  let SIM_SOPT2_RTCCLKOUTSEL_RTC_1 $bool = yes // RTC 32.768kHz clock is output on the RTC_CLKOUT pin.

