  let UART2_S2_LBKDE_Break_0 $bool = no // Break character detection is disabled.
  let UART2_S2_LBKDE_Break_1 $bool = yes // Break character is detected at length of 11 bit times if C1[M] = 0 or 12 bits time if C1[M] = 1.

