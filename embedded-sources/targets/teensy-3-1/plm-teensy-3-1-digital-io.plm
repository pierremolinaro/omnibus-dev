public func digitalWrite @noUnusedWarning (?port:port $digitalPort
                                           ?value $bool) {
  switch port {
  case D0 : // PTB16
    if value {
      GPIOB_PSOR = 1 << 16 ;
    }else{
      GPIOB_PCOR = 1 << 16 ;
    }
  case D1 : // PTB17
    if value {
      GPIOB_PSOR = 1 << 17 ;
    }else{
      GPIOB_PCOR = 1 << 17 ;
    }
  case D2 : // PTD0
    if value {
      GPIOD_PSOR = 1 << 0 ;
    }else{
      GPIOD_PCOR = 1 << 0 ;
    }
  case D3 : // PTA12
