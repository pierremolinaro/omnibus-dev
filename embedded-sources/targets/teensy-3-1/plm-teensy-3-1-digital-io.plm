  case D11 : // PTC6
    GPIOC_PTOR = 1 << 6 ;
  case D12 : // PTC7
    GPIOC_PTOR = 1 << 7 ;
  case D13 : // PTC5
    GPIOC_PTOR = 1 << 5 ;
  case D14 : // PTD1
    GPIOD_PTOR = 1 << 1 ;
  case D15 : // PTC0
    GPIOC_PTOR = 1 << 0 ;
  case D16 : // PTB0
    GPIOB_PTOR = 1 << 0 ;
  case D17 : // PTB1
    GPIOB_PTOR = 1 << 1 ;
  case D18 : // PTB3
    GPIOB_PTOR = 1 << 3 ;
  case D19 : // PTB2
    GPIOB_PTOR = 1 << 2 ;
  case D20 : // PTD5
