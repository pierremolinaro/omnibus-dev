public func toggle @noUnusedWarning (?port:port $digitalPort) {
  switch port {
  case D0 : // PTB16
    GPIOB_PTOR = 1 << 16 ;
  case D1 : // PTB17
    GPIOB_PTOR = 1 << 17 ;
  case D2 : // PTD0
    GPIOD_PTOR = 1 << 0 ;
  case D3 : // PTA12
    GPIOA_PTOR = 1 << 12 ;
  case D4 : // PTA13
    GPIOA_PTOR = 1 << 13 ;
  case D5 : // PTD7
    GPIOD_PTOR = 1 << 7 ;
  case D6 : // PTD4
    GPIOD_PTOR = 1 << 4 ;
  case D7 : // PTD2
    GPIOD_PTOR = 1 << 2 ;
  case D8 : // PTD3
