public func pinMode @noUnusedWarning (?port:port $digitalPort
                                      ?mode:mode $digitalMode) {
  switch port {
  case D0 : // PTB16
    switch mode {
    case OUTPUT :
      PORTB_PCR [16] = $PORTB_PCR {MUX:1}
      GPIOB_PDDR |= (1 << 16)
    case INPUT :
      PORTB_PCR [16] = $PORTB_PCR {MUX:1}
      GPIOB_PDDR &= ~ (1 << 16)
    case INPUT_PULLUP :
      PORTB_PCR [16] = $PORTB_PCR {MUX:1, PE, PS}
      GPIOB_PDDR &= ~ (1 << 16)
    }
  case D1 : // PTB17
    switch mode {
    case OUTPUT :
      PORTB_PCR [17] = $PORTB_PCR {MUX:1}
      GPIOB_PDDR |= (1 << 17)
    case INPUT :
