        GPIOC.PDDR &= ~ (1 << 7)
      }
    case D13 : // PTC5
      switch mode {
      case OUTPUT :
        PORTC.PCR [5] = {PORTC.PCR !MUX:1}
        GPIOC.PDDR |= (1 << 5)
      case OUTPUT_OPEN_COLLECTOR :
        PORTC.PCR [5] = {PORTC.PCR !MUX:1 !ODE:1}
        GPIOC.PDDR |= (1 << 5)
      case INPUT :
        PORTC.PCR [5] = {PORTC.PCR !MUX:1}
        GPIOC.PDDR &= ~ (1 << 5)
      case INPUT_PULLUP :
        PORTC.PCR [5] = {PORTC.PCR !MUX:1 !PE:1 !PS:1}
        GPIOC.PDDR &= ~ (1 << 5)
      }
    case D14 : // PTD1
      switch mode {
      case OUTPUT :
        PORTD.PCR [1] = {PORTD.PCR !MUX:1}
        GPIOD.PDDR |= (1 << 1)
      case OUTPUT_OPEN_COLLECTOR :
        PORTD.PCR [1] = {PORTD.PCR !MUX:1 !ODE:1}
        GPIOD.PDDR |= (1 << 1)
      case INPUT :
        PORTD.PCR [1] = {PORTD.PCR !MUX:1}
        GPIOD.PDDR &= ~ (1 << 1)
      case INPUT_PULLUP :
        PORTD.PCR [1] = {PORTD.PCR !MUX:1 !PE:1 !PS:1}
        GPIOD.PDDR &= ~ (1 << 1)
      }
    case D15 : // PTC0
      switch mode {
      case OUTPUT :
        PORTC.PCR [0] = {PORTC.PCR !MUX:1}
        GPIOC.PDDR |= (1 << 0)
      case OUTPUT_OPEN_COLLECTOR :
        PORTC.PCR [0] = {PORTC.PCR !MUX:1 !ODE:1}
        GPIOC.PDDR |= (1 << 0)
      case INPUT :
        PORTC.PCR [0] = {PORTC.PCR !MUX:1}
        GPIOC.PDDR &= ~ (1 << 0)
      case INPUT_PULLUP :
        PORTC.PCR [0] = {PORTC.PCR !MUX:1 !PE:1 !PS:1}
        GPIOC.PDDR &= ~ (1 << 0)
      }
    case D16 : // PTB0
      switch mode {
      case OUTPUT :
        PORTB.PCR [0] = {PORTB.PCR !MUX:1}
        GPIOB.PDDR |= (1 << 0)
      case OUTPUT_OPEN_COLLECTOR :
        PORTB.PCR [0] = {PORTB.PCR !MUX:1 !ODE:1}
        GPIOB.PDDR |= (1 << 0)
      case INPUT :
        PORTB.PCR [0] = {PORTB.PCR !MUX:1}
        GPIOB.PDDR &= ~ (1 << 0)
      case INPUT_PULLUP :
        PORTB.PCR [0] = {PORTB.PCR !MUX:1 !PE:1 !PS:1}
        GPIOB.PDDR &= ~ (1 << 0)
      }
    case D17 : // PTB1
      switch mode {
      case OUTPUT :
        PORTB.PCR [1] = {PORTB.PCR !MUX:1}
        GPIOB.PDDR |= (1 << 1)
      case OUTPUT_OPEN_COLLECTOR :
        PORTB.PCR [1] = {PORTB.PCR !MUX:1 !ODE:1}
        GPIOB.PDDR |= (1 << 1)
      case INPUT :
        PORTB.PCR [1] = {PORTB.PCR !MUX:1}
        GPIOB.PDDR &= ~ (1 << 1)
      case INPUT_PULLUP :
