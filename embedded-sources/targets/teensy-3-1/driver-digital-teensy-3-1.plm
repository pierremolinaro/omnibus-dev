      case OUTPUT_OPEN_COLLECTOR :
        PORTB.PCR [2] = {PORTB.PCR !MUX:1 !ODE:1}
        GPIOB.PDDR |= (1 << 2)
      case INPUT :
        PORTB.PCR [2] = {PORTB.PCR !MUX:1}
        GPIOB.PDDR &= ~ (1 << 2)
      case INPUT_PULLUP :
