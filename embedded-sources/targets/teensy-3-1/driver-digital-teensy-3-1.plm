        GPIOD.PDDR &= ~ (1 << 5)
      case INPUT_PULLUP :
        PORTD.PCR [5] = {PORTD.PCR !MUX:1 !PE:1 !PS:1}
        GPIOD.PDDR &= ~ (1 << 5)
      }
    case D21 : // PTD6
      switch mode {
      case OUTPUT :
        PORTD.PCR [6] = {PORTD.PCR !MUX:1}
        GPIOD.PDDR |= (1 << 6)
      case OUTPUT_OPEN_COLLECTOR :
        PORTD.PCR [6] = {PORTD.PCR !MUX:1 !ODE:1}
        GPIOD.PDDR |= (1 << 6)
