    case D18 : // PTB3
      switch mode {
      case OUTPUT :
        PORTB.PCR [3] = {PORTB.PCR !MUX:1}
        GPIOB.PDDR |= (1 << 3)
      case OUTPUT_OPEN_COLLECTOR :
        PORTB.PCR [3] = {PORTB.PCR !MUX:1 !ODE:1}
        GPIOB.PDDR |= (1 << 3)
      case INPUT :
