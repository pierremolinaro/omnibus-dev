// Chapter 46: Periodic Interrupt Timer (PIT)
//——————————————————————————————————————————————————————————————————————————————————————————————————————————————————————

let PIT_COUNT = 4

registers PIT {
  MCR 0x4003_7000 $uint32 {30, MDIS, FRZ}
  
  // PIT Lifetime Timer Registers
  LTMR64H @ro 0x4003_70E0 $uint32
  LTMR64L @ro 0x4003_70E4 $uint32
    
  // Timer Load Value Register
  LDVAL [PIT_COUNT] 0x4003_7100 : 16 $uint32
  
  // Current Timer Value Register
  LCVAL [PIT_COUNT] 0x4003_7104 : 16 $uint32
  
  // Timer Control Register
  TCTRL [PIT_COUNT] 0x4003_7108 : 16 $uint32 {29, CHN, TIE, TEN}
  
  // Timer Flag Register
  TFLG [PIT_COUNT] 0x4003_710C : 16 $uint32 {31, TIF}

}

