  PLL
  RTC
  EINT0
  EINT1
  EINT2
  EINT3
  ADC
  CAN_COMMON
  CAN0_TX
  CAN1_TX
  CAN2_TX
  CAN3_TX
  FULLCAN
  CAN0_RX
  CAN1_RX
  CAN2_RX
  CAN3_RX
  
  PAbort : 40
  DAbort : 41
  FIQ : 42
}

