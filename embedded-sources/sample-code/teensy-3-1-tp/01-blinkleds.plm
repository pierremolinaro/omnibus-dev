target "teensy-3-1-tp"

//------------------------------------------------*

task T1 priority 1 stackSize 512 {
  var compteur $uint32 = 0

  proc loop () {
    waitUntilMS (!deadline:self.compteur)
    ledOn (!LED_L0)
    self.compteur +%= 500
    waitUntilMS (!deadline:self.compteur)
    ledOff (!LED_L0)
    self.compteur +%= 500
    goto (!line:0 !column:0)
    printUnsigned (!millis ())
  }
}

