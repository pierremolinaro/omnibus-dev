target "teensy-3-1-xtr"

//------------------------------------------------*

task BlinkLed {
  init {
    PORTC_PCR5 = PORTC_PCR5::mux (1)
    GPIOC_PDDR |= (1 << 5)
  }

  var state : $bool = true
  
  every (!ms:500) {
    if state then
      GPIOC_PSOR = 1 << 5 // Allumer la led
      state = false
    else
      GPIOC_PCOR = 1 << 5 // Ã‰teindre la led
      static = true
    end  
  }

//------------------------------------------------*
