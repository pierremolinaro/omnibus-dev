target "teensy-3-1-it"

//------------------------------------------------*

struct $MyStruct {
  var x $int32 = 9
  var y $int32 = 90

  proc nop `user () {
  }
  proc getX `user (!outX $int32) {
    outX = self.x %+ 3
  }
  mutating proc setX `user (?inX $int32) {
    self.x = inX + 8
  }
}

//------------------------------------------------*

proc setup `user () {
}

//------------------------------------------------*

proc loop `user () {
  var s = $MyStruct ()
  s.nop ()
  var x $int32 ; s.getX (?x)
  s.setX (!999)
}

//------------------------------------------------*
