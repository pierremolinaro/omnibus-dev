target "teensy-3-1-it"

//------------------------------------------------*

struct $point {
  var x $uint32 = 200
  var y $uint32 = 500
}

struct $point3 {
  var p $point = $point ()
  var z $int32 = 3
  
}

//------------------------------------------------*

var zz $int32 = 0 {
  @rw proc loop
}

//------------------------------------------------*

proc setup `user () {
}

//------------------------------------------------*

proc loop `user () {
  var p = $point3 ()
  waitMS (!p.p.x)
  p.p.y = 340
  var q = $point ()
  ledOn (!LED_L1) // Allumer la led
  waitMS (!p.p.y)
  ledOff (!LED_L1)  // Ã‰teindre la led
}

