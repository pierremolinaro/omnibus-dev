target "teensy-3-1-it"

//------------------------------------------------*

proc setup `user () {
}

//------------------------------------------------*

var gDelai $uint32 = 0 {
  @rw proc loop
}

//------------------------------------------------*

proc loop `user () {
  gDelai +%= 1
  if gDelai == 1_000_000 then
    ledOn (!LED_TEENSY) // Allumer la led
  elsif gDelai == 2_000_000 then
    gDelai = 0
    ledOff (!LED_TEENSY)  // Ã‰teindre la led
  end  
}

//------------------------------------------------*
