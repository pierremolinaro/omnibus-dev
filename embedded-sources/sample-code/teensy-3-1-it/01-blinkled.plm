target "teensy-3-1-it"

//------------------------------------------------*

proc setup `user () {
}

newtype $u : $int32

//------------------------------------------------*

var gDelai $u = 0 {
  @rw proc loop
}

//------------------------------------------------*

proc loop `user () {
  gDelai +%= 1
  if gDelai == 1_000_000 then
    ledOn (!LED_TEENSY) // Allumer la led
  elsif 2_000_000 == gDelai then
    gDelai = 0
    ledOff (!LED_TEENSY)  // Ã‰teindre la led
  end  
}

//------------------------------------------------*
