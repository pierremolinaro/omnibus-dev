target "teensy-3-1-it"

//------------------------------------------------*

proc setup `user () {
}

newtype $u : $int32
let bound = 1000 * 1000 * 10

//------------------------------------------------*

var gDelai $u = 0 {
  @rw proc loop
}

var gVar $uint32 = 0 {
  @rw proc loop
}

//------------------------------------------------*

proc loop `user () {
  gDelai +%= 1
  if gDelai == bound then
    ledOn (!LED_TEENSY) // Allumer la led
  elsif (bound * 2) == gDelai then
    gDelai = 0
    ledOff (!LED_TEENSY)  // Ã‰teindre la led
    gVar +%= 1
    goto (!line:0 !column:0)
    printUnsigned (!gVar)
    goto (!line:1 !column:0)
    printUnsigned (!gVar !% 20)
  end  
}

//------------------------------------------------*
