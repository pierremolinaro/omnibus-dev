target "teensy-3-1-it"

//------------------------------------------------*

proc setup `user () {
}

//------------------------------------------------*

var toggle = true {
  @rw proc loop
}

var gCount $uint32 = 0 {
  @rw proc loop
}

//------------------------------------------------*

proc loop `user () {
  waitMS (!250)
  ledOn (!LED_TEENSY) // Allumer la led
  waitMS (!250)
  ledOff (!LED_TEENSY)  // Ã‰teindre la led
  toggle = not toggle
  if toggle then
    gCount += 1
    goto (!line:1 !column:8)
    printUnsigned (!gCount)
  end
}

//------------------------------------------------*
