target "teensy-3-1-it"

//------------------------------------------------*

enum $ledState {
  case on
  case off
}

//------------------------------------------------*

proc setup `user () {
}

//------------------------------------------------*

var led = $ledState.on {
  @rw proc loop
}

//------------------------------------------------*

proc loop `user () {
  waitMS (!250)
  if led == .off then
    ledOn (!LED_TEENSY) // Allumer la led
    led = .on
  else
    ledOff (!LED_TEENSY)  // Ã‰teindre la led
    led = .off
  end
}

//------------------------------------------------*
