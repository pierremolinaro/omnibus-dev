target "teensy-3-1-sequential-systick"

//------------------------------------------------*

struct $point {
  var x $uint32 = 200
  var y $uint32 = 500
}

struct $point3 {
  var p $point = $point ()
  var z $int32 = 3
}

//------------------------------------------------*

var zz $int32 = 0 {
  @rw proc loop ()
}

//------------------------------------------------*

proc setup `user () {
}

//------------------------------------------------*

proc loop `user () {
  var p = $point3 ()
  waitMS (!p.p.x)
  p.p.y = 340
//  zz = zzz.p.x
//  zzz.p.x = zz
//  zzz = $point3 ()
  ledOn (!LED_TEENSY) // Allumer la led
  waitMS (!p.p.y)
  ledOff (!LED_TEENSY)  // Ã‰teindre la led
}

//------------------------------------------------*
