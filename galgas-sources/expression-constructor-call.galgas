override method @constructorCallAST solveEntities
  ?globalConstants:@globalConstantMap inGlobalConstantMap
  ?registers:let @controlRegisterMap inRegisterMapSE
  ?globalVars:let @globalVariableMap inGlobalVariableMapSE
  !@expressionSE outExpression
{
  @functionCallEffectiveParameterListSE parameterList = {}
  for (selector expressionAST) in mParameterList do
    [expressionAST solveEntities
      !globalConstants:inGlobalConstantMap
      !registers:inRegisterMapSE
      !globalVars:inGlobalVariableMapSE
      ?let expression
    ]
    parameterList += !selector !expression
  end
  outExpression = @constructorCallSE.new {!mTypeName !parameterList !mErrorLocation}
}

