  %void%
else
  for () in FORMAL_ARGUMENT_LIST
  do
    !mangledNameForType (!name:[mFormalArgumentType key])
    % %!mFormalArgumentName
  between
    %,\n%!^
  end
end
%) {
  %!mangledNameForType (!name:[RESULT_TYPE key])% %!RESULT_VAR_NAME% ;
%![INSTRUCTION_GENERATION_LIST instructionListCode]
