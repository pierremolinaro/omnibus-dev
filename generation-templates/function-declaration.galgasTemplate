%!typePrefix () ![RESULT_TYPE key]% %!functionPrefix () ![FUNCTION_MANGLED_NAME identifierRepresentation]% (%?^
if [FORMAL_ARGUMENT_LIST length] == 0 then
  %void%
else
  for () in FORMAL_ARGUMENT_LIST
  do
    !typePrefix () ![mFormalArgumentType key]
    % %!mFormalArgumentName
  between
    %,\n%!^
  end
end
%) ;

