//---------------------------------------------------------------------------------------------------------------------*
//%!["proc " + FUNCTION_MANGLED_NAME stringByLeftAndRightPadding !117 !' ']%*
//---------------------------------------------------------------------------------------------------------------------*

%!mangledNameForType (!name:[RESULT_TYPE key])% %!FUNCTION_MANGLED_NAME% (%?^
if [FORMAL_ARGUMENT_LIST length] == 0 then
  %void%
else
  for () in FORMAL_ARGUMENT_LIST
  do
    !mangledNameForType (!name:[mFormalArgumentType key])
    % %!mFormalArgumentName
  between
    %,\n%!^
  end
end
%) ;

